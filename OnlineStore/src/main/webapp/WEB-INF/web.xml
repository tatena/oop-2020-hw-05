<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
        xmlns="java.sun.com/xml/ns/javaee"
        xmlns:xsi="w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="java.sun.com/xml/ns/javaee java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>home-servlet</servlet-name>
    <servlet-class>store.HomeServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>home-servlet</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>product-servlet</servlet-name>
    <servlet-class>store.ProductServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>product-servlet</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>shoppingCart-servlet</servlet-name>
    <servlet-class>store.ShoppingCartServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>shoppingCart-servlet</servlet-name>
    <url-pattern>/shoppingCart</url-pattern>
  </servlet-mapping>
</web-app>
